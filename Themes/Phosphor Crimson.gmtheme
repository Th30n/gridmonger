ui {
  window {
    backgroundColor = "#080002ff"
    backgroundImage

  title {
    normal = "#2d000dff"
    inactive = "#190300ff"
  }
  text {
    color = "#ff2d6aed"
    inactive = "#842541ff"
  }
  modifiedFlagColor = "#ff487e64"

  button {
    normal = "#b40035c8"
    hover = "#d2003dc8"
    down = "#f00046e8"
  }

  dialog {
    corner-radius = 0.0
    background = "#170007ff"
    labelColor = "#e00042ff"
    warningColor = "#ff4b7dff"

    title {
      backgroundColor = "#ca003cff"
      color = "#000000cc"
    }
    outer-border {
      color = "#0f0004ff"
      width = 6.0
    inner-border {
      color = "#ca003cff"
      width = 2.0
    }
    shadow {
      enabled = false
      x-offset = 0.0
      y-Offset = 0.0
      feather = 0.0
      color = "#00000000"
    }

  widget {
    corner-radius = 3.0
    background {
      normal = "#a4003027"
      hover = "#ff004b3c"
      active = "#ff004b6f"
      disabled = "#a400302a"
    }
    foreground { 
      normal = "#ff004bff"
      active = "#ff457aff"
      disabled = "#a4003080"
    }

  text-field {
    cursorColor = "#ffbe00ff"
    selectionColor = "#ff3e772d"
    edit {
      backgroundColor = "#ff004b1e"
      textColor = "#f5618cf0"
    }
    scroll-bar {
      normal = "#ff004b1e"
      edit = "#c14c6ef0"
    }
  }

  status-bar {
    backgroundColor = "#2d000dff"
    textColor = "#ff2d6adc"
    coordinatesColor = "#ff2d6ab4"
    command {
      backgroundColor = "#d1335fe2"
      color = "#190300ff"
    }
  }

  about-button {
    label {
      normal = "#c84600ff"
      hoverColor = "#eb6400ff"
      downColor = "#f5b400ff"
    }
  }

  about-dialog {
    logoColor = "#ff2d6ab4"
  }

  splash-image {
    logoColor = "#0a0003ff"
    outlineColor = "#e0003fff"
    shadow-alpha = 1.0
  }
}

[level.general]
backgroundColor = "#080002ff"
foregroundColor = "#e0003fff"
foregroundLightColor = "#64001dff"
lineWidth = Thin
coordinatesColor = "#6d001fff"
coordinatesHighlightColor = "#ff0049ff"
cursorColor = "#ffb3006e"
cursorGuidesColor = "#ffb4c92c"
selectionColor = "#a95d2a66"
pastePreviewColor = "#5789da69"
linkMarkerColor = "#f38800ff"
trailColor = "#e0003f6d"
regionBorderColor = "#ff91b0ff"
regionBorderEmptyColor = "#ff91b05e"

[level.backgroundHatch]
enabled = true
color = "#200009ff"
width = 2.0
spacingFactor = 4.0

[level.grid]
backgroundStyle = None
backgroundGridColor = "#00000000"
floorStyle = Loose
floorGridColor = "#510016ff"

[level.outline]
style = Cell
fillStyle = Solid
color = "#200009ff"
widthFactor = 0.0
overscanEnabled = false

[level.shadow]
innerColor = "#00000000"
innerWidthFactor = 0.0
outerColor = "#00000000"
outerWidthFactor = 0.0

[level.floorColor]
