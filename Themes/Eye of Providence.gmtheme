ui {
  window {
    modified-flag = "#d2c3a978"

    border {
      color = "#1b1b1bff"
    }
    background {
      color = "#a2843dff"
      image = ""
    }
    title {
      background {
        normal = "#30281bff"
        inactive = "#1a1a1aff"
      }
      text {
        normal = "#d2c3a9b4"
        inactive = "#ffffff66"
      }
    }
    button {
      normal = "#aca45896"
      hover = "#aca458c8"
      down = "#aca458ff"
      inactive = "#00000080"
    }
  }

  dialog {
    corner-radius = 5
    background = "#443e30ff"
    label = "#bfb6a1ff"
    warning = "#ffffffec"
    error = "#ff6464ff"

    title {
      background = "#221f18ff"
      text = "#dad0b7ff"
    }
    inner-border {
      color = "#00000000"
      width = 0
    }
    outer-border {
      color = "#00000000"
      width = 0
    }
    shadow {
      enabled = no
      color = "#00000000"
      feather = 0
      x-offset = 0
      y-offset = 0
    }
  }

  widget {
    corner-radius = 3

    background {
      normal = "#a49c89ff"
      hover = "#cdc2abff"
      active = "#ffb100ff"
      disabled = "#b0a79355"
    }
    foreground {
      normal = "#000000b3"
      active = "#0000009a"
      disabled = "#00000059"
    }
  }

  text-field {
    cursor = "#ffb100ff"
    selection = "#c8820078"

    edit {
      background = "#ffffff33"
      text = "#dad0b7ff"
    }
    scroll-bar {
      normal = "#00000000"
      edit = "#ffffff00"
    }
  }

  status-bar {
    background = "#3e3428ff"
    text = "#d6ccc0e6"
    coordinates = "#ad9964eb"

    command {
      background = "#9a793aff"
      text = "#3e3428ff"
    }
  }

  about-button {
    label {
      normal = "#00000066"
      hover = "#00000080"
      down = "#00000099"
    }
  }

  about-dialog {
    logo = "#eaeaeaff"
  }

  splash-image {
    logo = "#3c362eff"
    outline = "#d3c7bbff"
    shadow-alpha = 0.4903846153846154
  }
}

level {
  general {
    background = "#a2843dff"
    line-width = normal
    cursor = "#ffa600ff"
    cursor-guides = "#ffa60033"
    link-marker = "#7a592099"
    selection = "#ff808066"
    trail = "#4d1c3457"
    paste-preview = "#3399ff66"

    foreground {
      normal = "#594840ff"
      light = "#5948405c"
      normal-cursor = "#594840ff"
      light-cursor = "#5948405c"
    }
    coordinates {
      normal = "#00000066"
      highlight = "#000000cc"
    }
    region-border {
      normal = "#924b30ff"
      empty = "#924b306f"
    }
  }

  background-hatch {
    enabled = no
    color = "#00000000"
    width = 1
    spacing-factor = 3
  }

  grid {
    background {
      style = none
      grid = "#00000000"
    }
    floor {
      style = solid
      grid = "#0303035c"
    }
  }

  outline {
    style = none
    fill-style = solid
    color = "#00000000"
    width-factor = 0
    overscan = no
  }

  shadow {
    inner {
      color = "#00000000"
      width-factor = 0
    }
    outer {
      color = "#00000000"
      width-factor = 0
    }
  }

  floor {
    transparent = no
    background = [
      "#eeeeeeff"
      "#9d3336bb"
      "#a2333673"
      "#a29743a7"
      "#a26533bb"
      "#5f9143e5"
      "#71a233bb"
      "#55b5ed8c"
      "#3379a2bb"
      "#a23373b9"
    ]
  }

  note {
    marker = "#3c2c1cbe"
    comment = "#ff3c00ab"
    background-shape = rectangle
    index = "#000000c1"
    index-background = [
      "#14ed0eff"
      "#00f8f1ff"
      "#ff5375ff"
      "#c87dffff"
      "#493929ff"
    ]

    tooltip {
      background = "#fffee8fa"
      text = "#3c2c1cff"
      corner-radius = 5

      shadow {
        color = "#00000064"
      }
    }

    marker-cursor = "#3c2c1cbe"
  }

  label {
    text = [
      "#5d513cff"
      "#ffe7bedd"
      "#9c632dff"
      "#ffb635e4"
    ]
  }

  level-drop-down {
    item-list-background = "#3c3422ff"
    corner-radius = 5

    button {
      normal = "#ffffff00"
      hover = "#ffffff26"
      label = "#000000af"
    }
    item {
      normal = "#e6cb99fa"
      hover = "#3c3422e6"
    }
    shadow {
      color = "#00000064"
    }
  }
}

pane {
  notes {
    text = "#000000af"
    scroll-bar = "#d3c5b8ff"
    index = "#000000b5"
    index-background = [
      "#12da0dff"
      "#00d3cdff"
      "#ff5375ff"
      "#c87dffff"
    ]
  }

  toolbar {
    button {
      normal = "#d8bc90d3"
      hover = "#f2d6abff"
    }
  }
}

