      backgroundColor = "#ffffff33"
      textColor = "#ffffffca"
    }
    scroll-bar {
      normal = "#00000000"
      edit = "#ffffff00"
    }
  }

  status-bar {
    backgroundColor = "#191b20ff"
    textColor = "#ccccccff"
    coordinatesColor = "#ffffff6a"
    command {
      backgroundColor = "#777a83ff"
      color = "#202228ff"
    }
  }

  about-button {
    label {
      normal = "#ffffff80"
      hoverColor = "#ffffffb3"
      downColor = "#ffffffd9"
    }
  }

  about-dialog {
    logoColor = "#babfc3ff"
  }

  splash-image {
    logoColor = "#25282fff"
    outlineColor = "#babfc3ff"
    shadow-alpha = 1.0
  }
}

