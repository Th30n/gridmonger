    }
    shadow {
      enabled = false
      x-offset = 0.0
      y-Offset = 0.0
      feather = 0.0
      color = "#00000000"
    }

  widget {
    corner-radius = 4.0
    background {
      normal = "#62bec53c"
      hover = "#6fe0db50"
      active = "#00d9e0ff"
      disabled = "#6fd8e026"
