    scroll-bar {
      normal = "#6ed6dd28"
      edit = "#7ebdc1ff"
    }
  }

  status-bar {
    backgroundColor = "#001c1eff"
    textColor = "#00bcc9ff"
    coordinatesColor = "#00b2bfff"
    command {
      backgroundColor = "#10939cff"
      color = "#001f22ff"
    }
  }

  about-button {
    label {
      normal = "#00bcc977"
      hoverColor = "#00bcc9a4"
      downColor = "#00bcc9d0"
    }
  }

  about-dialog {
    logoColor = "#00b2bfff"
  }

  splash-image {
    logoColor = "#000808ff"
    outlineColor = "#00d0e0ff"
    shadow-alpha = 0.8173076923076923
  }
}

