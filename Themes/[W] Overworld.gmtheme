ui {
  window {
    modified-flag = "#ffffff40"

    background {
      color = "#62584eff"
      image = ""
    }
    title {
      background {
        normal = "#2e2e2eff"
        inactive = "#1a1a1aff"
      }
      text {
        normal = "#ffffff80"
        inactive = "#ffffff66"
      }
    }
    button {
      normal = "#ffffff73"
      hover = "#ffffffb3"
      down = "#ffffffe6"
    }
  }

  dialog {
    corner-radius = 8.0
    background = "#404040ff"
    label = "#ffffffb3"
    warning = "#ff6464ff"

    title {
      background = "#1a1a1aff"
      text = "#d9d9d9ff"
    }
    outer-border {
      color = "#00000000"
      width = 0.0
    }
    inner-border {
      color = "#00000000"
      width = 0.0
    }
    shadow {
      enabled = no
      color = "#00000000"
      feather = 0.0
      x-offset = 0.0
      y-offset = 0.0
    }
  }

  widget {
    corner-radius = 5.0

    background {
      normal = "#ffffff80"
      hover = "#ffffffa1"
      active = "#ff8b00ff"
      disabled = "#ffffff33"
    }
    foreground {
      normal = "#000000b3"
      active = "#000000a7"
      disabled = "#00000059"
    }
  }

  text-field {
    cursor = "#ffbe00ff"
    selection = "#c8820078"

    edit {
      background = "#ffffff33"
      text = "#ffffffb3"
    }
    scroll-bar {
      normal = "#00000000"
      edit = "#00000000"
    }
  }

  status-bar {
    background = "#333333ff"
    text = "#ccccccff"
    coordinates = "#999999ff"

    command {
      background = "#808080ff"
      text = "#262626ff"
    }
  }

  about-button {
    label {
      normal = "#00000066"
      hover = "#00000066"
      down = "#000000b3"
    }
  }

  about-dialog {
    logo = "#00000000"
  }

  splash-image {
    logo = "#604745ff"
    outline = "#ffeca5ff"
    shadow-alpha = 1.0
  }
}

level {
  general {
    background = "#62584eff"
    cursor = "#ff8b00ff"
    cursor-guides = "#ffb30033"
    line-width = Normal
    link-marker = "#00b3c8ff"
    selection = "#ff808080"
    trail = "#00000051"
    paste-preview = "#3399ff80"

    foreground {
      normal = "#222022ff"
      light = "#2d2a2a49"
    }
    coordinates {
      normal = "#ffe69996"
      highlight = "#ffe164ff"
    }
    region-border {
      normal = "#00000000"
      empty = "#00000000"
    }
  }

  background-hatch {
    enabled = no
    color = "#00000000"
    width = 0.5
    spacing-factor = 1.0
  }

  grid {
    background {
      style = None
      grid = "#00000000"
    }
    floor {
      style = solid
      grid = "#0000001a"
    }
  }

  outline {
    style = none
    fill-style = solid
    color = "#00000000"
    width-factor = 0.0
    overscan = no
  }

  shadow {
    inner {
      color = "#00000000"
      width-factor = 0.0
    }
    outer {
      color = "#00000000"
      width-factor = 0.0
    }
  }

  floor {
    transparent = no
    background = [
      "#ebd48dff"
      "#ffd478ff"
      "#bb9b59ff"
      "#5ba130ff"
      "#517819ff"
      "#1faadfff"
      "#ffbd48ff"
      "#7b5b58ff"
      "#a99c9cff"
      "#696161ff"
    ]
  }

  note {
    marker = "#191616b4"
    comment = "#ff3300cc"
    background-shape = circle
    index = "#ffffffff"
    index-background = [
      "#fd4948ff"
      "#32b6e5ff"
      "#a0a2a2ff"
      "#4c4e4eff"
    ]

    tooltip {
      background = "#000000e8"
      text = "#ffffffe6"
    }
  }

  label {
    text = [
      "#5d513cff"
      "#ffe7bedd"
      "#9c632dff"
      "#ffb635e4"
    ]
  }

  level-drop-down {
    item-list-background = "#404040ff"

    button {
      normal = "#00000000"
      hover = "#0000001a"
      label = "#ffe6beb4"
    }
    item {
      normal = "#ffffffcc"
      hover = "#000000b3"
    }
  }
}

pane {
  notes {
    text = "#ffeeeed4"
    scroll-bar = "#00000000"
    index = "#ffffffff"
    index-background = [
      "#fd4948ff"
      "#1e98c7ff"
      "#878989ff"
      "#4c4e4eff"
    ]
  }

  toolbar {
    button {
      normal = "#ffe6b496"
      hover = "#fffffcff"
    }
  }
}

