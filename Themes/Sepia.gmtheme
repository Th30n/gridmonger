ui {
  window {
    backgroundColor = "#cebeafff"
    backgroundImage

  title {
    normal = "#30281bff"
    inactive = "#1a1a1aff"
  }
  text {
    color = "#d2c3a9b4"
    inactive = "#ffffff66"
  }
  modifiedFlagColor = "#d2c3a978"

  button {
    normal = "#aca45896"
    hover = "#aca458c8"
    down = "#aca458ff"
  }

  dialog {
    corner-radius = 0.0
    background = "#cebeafff"
    labelColor = "#0000008e"
    warningColor = "#c12409a9"

    title {
      backgroundColor = "#0d0d0dc8"
      color = "#d9d9d9ff"
    }
    outer-border {
      color = "#cebeafff"
      width = 0.0
    inner-border {
      color = "#93887dff"
      width = 7.0
    }
    shadow {
      enabled = false
      x-offset = 0.0
      y-Offset = 0.0
      feather = 0.0
      color = "#00000000"
    }

  widget {
    corner-radius = 5.0
    background {
      normal = "#70645dd8"
      hover = "#8d7568d8"
      active = "#ffd8aaff"
      disabled = "#70645d68"
    }
    foreground { 
      normal = "#fff0f0e9"
      active = "#0000006f"
      disabled = "#fff2f25e"
    }

  text-field {
    cursorColor = "#ffbe00ff"
    selectionColor = "#ffce9445"
    edit {
      backgroundColor = "#0000007b"
      textColor = "#ffffffed"
    }
    scroll-bar {
      normal = "#ffe1be45"
      edit = "#ffffffed"
    }
  }

  status-bar {
    backgroundColor = "#3e3428ff"
    textColor = "#d6ccc0e6"
    coordinatesColor = "#ad9964eb"
    command {
      backgroundColor = "#9a793aff"
      color = "#3e3428ff"
    }
  }

  about-button {
    label {
      normal = "#00000066"
      hoverColor = "#00000080"
      downColor = "#00000099"
    }
  }

  about-dialog {
    logoColor = "#3e3428c3"
  }

  splash-image {
    logoColor = "#444341ff"
    outlineColor = "#eeeae6ff"
    shadow-alpha = 0.5
  }
}

[level.general]
backgroundColor = "#cebeafff"
foregroundColor = "#3f3f3bff"
foregroundLightColor = "#3f3f3b6e"
lineWidth = Normal
coordinatesColor = "#00000066"
coordinatesHighlightColor = "#000000cc"
cursorColor = "#ffa600ff"
