color1 = "#d9d9d5ff"
color2 = "#6f000097"
color3 = "#ff290074"
color4 = "#ffb30080"
color5 = "#7c652192"
color6 = "#709a0092"
color7 = "#a6c41e63"
color8 = "#00a36c6f"
color9 = "#1c6fac83"
color10 = "#9c559fa6"

[level.note]
